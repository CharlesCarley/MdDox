
digraph {
     bgcolor = none;
     rankdir = "LR";
     node [
         shape     = none
         fontname  = "Segoe UI"
         fontcolor = "#010509FF"
         fontsize  = "11"
     ]
     edge [
         arrowsize = "0.5"
         color     = "#FF1040FF"
     ]
     SiteBuilder->Config[dir=back];

     {rank=same Config SiteBuilder}

     SiteBuilder->buildFromXml
     buildFromXml->Build
     buildFromXml->WriteIndex
     buildFromXml->Dispatch
     buildFromXml->ConvertGraphs
     Dispatch->BuildItem
     Dispatch->WriteItem
}
